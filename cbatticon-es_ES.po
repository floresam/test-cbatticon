#: cbatticon.c:163
msgid "Display the version"
msgstr "Mostrar la versións"

#: cbatticon.c:164
msgid "Display debug information"
msgstr "Mostrar información de depuración"

#: cbatticon.c:165
msgid "Set update interval (in seconds)"
msgstr "Establecer intervalo de actualización (en segundos)"

#: cbatticon.c:166
msgid "Set icon type ('standard', 'notification' or 'symbolic')"
msgstr "Establecer tipo de icono (“estándar”, “notificación” o “simbólico”)"

#: cbatticon.c:167
msgid "Set low battery level (in percent)"
msgstr "Establecer nivel de batería baja (en porcentaje)"

#: cbatticon.c:168
msgid "Set critical battery level (in percent)"
msgstr "Establecer nivel de batería crítico (en porcentaje)"

#: cbatticon.c:169
msgid "Command to execute when low battery level is reached"
msgstr "Comando a ejecutar cuando se alcance el nivel de batería baja"

#: cbatticon.c:170
msgid "Command to execute when critical battery level is reached"
msgstr "Comando a ejecutar cuando se alcance el nivel de batería crítico"

#: cbatticon.c:171
msgid "Command to execute when left clicking on tray icon"
msgstr "Comando a ejecutar al hacer clic izquierdo en el icono de la bandeja del sistema"

#: cbatticon.c:173
msgid "Hide the notification popups"
msgstr "Ocultar las ventanas emergentes de notificación"

#: cbatticon.c:175
msgid "List available icon types"
msgstr "Listar tipos de iconos disponibles"

#: cbatticon.c:176
msgid "List available power supplies (battery and AC)"
msgstr "Listar fuentes de alimentación disponibles (batería y corriente alterna)"

#: cbatticon.c:180
msgid "[BATTERY ID]"
msgstr "[IDENTIFICADOR DE BATERÍA]"

#: cbatticon.c:184
#, c-format
msgid "Cannot parse command line arguments: %s\n"
msgstr "No se pueden analizar los argumentos de la línea de comandos: %s\n"

#: cbatticon.c:195
msgid ""
"cbatticon: a lightweight and fast battery icon that sits in your system "
"tray\n"
msgstr "cbatticon: un icono de batería ligero y rápido que se ubica en la bandeja del sistema\n"

#: cbatticon.c:196
#, c-format
msgid "version %s\n"
msgstr "versión %s\n"

#: cbatticon.c:204
msgid "List of available power supplies:\n"
msgstr "Lista de fuentes de alimentación disponibles:\n"

#: cbatticon.c:219
msgid "List of available icon types:\n"
msgstr "Lista de tipos de iconos disponibles:\n"

#: cbatticon.c:220 cbatticon.c:221 cbatticon.c:222
msgid "available"
msgstr "disponible"

#: cbatticon.c:220 cbatticon.c:221 cbatticon.c:222
msgid "unavailable"
msgstr "no disponible"

#: cbatticon.c:236
#, c-format
msgid "Unknown icon type: %s\n"
msgstr "Tipo de icono desconocido: %s\n"

#: cbatticon.c:248
msgid "No icon type found!\n"
msgstr "¡No se ha encontrado ningún tipo de icono!\n"

#: cbatticon.c:255
#, c-format
msgid "Invalid update interval! It has been reset to default (%d seconds)\n"
msgstr "¡Intervalo de actualización no válido! Se ha restablecido al valor predeterminado (%d segundos)\n"

#: cbatticon.c:262
#, c-format
msgid "Invalid low level! It has been reset to default (%d percent)\n"
msgstr "¡Nivel bajo no válido! Se ha restablecido al valor predeterminado (%d por ciento)\n"

#: cbatticon.c:267
#, c-format
msgid "Invalid critical level! It has been reset to default (%d percent)\n"
msgstr "¡Nivel crítico no válido! Se ha restablecido al valor predeterminado (%d por ciento)\n"

#: cbatticon.c:273
msgid ""
"Critical level is higher than low level! They have been reset to default\n"
msgstr "¡El nivel crítico es superior al nivel bajo! Se han restablecido a los valores predeterminados\n"

#: cbatticon.c:367 cbatticon.c:402
#, c-format
msgid "type: %-*.*s\tid: %-*.*s\tpath: %s\n"
msgstr "tipo: %%-*.*s\tid: %%-*.*s\truta: %s\n"

#: cbatticon.c:367
msgid "Battery"
msgstr "Batería"

#: cbatticon.c:402
msgid "AC"
msgstr "Corriente alterna"

#: cbatticon.c:424
#, c-format
msgid "Cannot open sysfs directory: %s (%s)\n"
msgstr "No se puede abrir el directorio sysfs: %s (%s)\n"

#: cbatticon.c:431
#, c-format
msgid "No battery with suffix %s found!\n"
msgstr "¡No se ha encontrado ninguna batería con el sufijo %s!\n"

#: cbatticon.c:436
msgid "No battery nor AC power supply found!\n"
msgstr "¡No se ha encontrado ninguna batería ni fuente de alimentación de corriente alterna!\n"

#: cbatticon.c:784 cbatticon.c:786
msgid "AC only, no battery!"
msgstr "Solo corriente alterna, ¡sin batería!"

#: cbatticon.c:910
#, c-format
msgid "Spawning low battery level command in 5 seconds: %s"
msgstr "Ejecutando el comando de nivel de batería baja en 5 segundos: %s"

#: cbatticon.c:915
msgid "Skipping low battery level command, no longer discharging"
msgstr "Omitiendo el comando de nivel de batería baja, ya no se está descargando"

#: cbatticon.c:921 cbatticon.c:923
#, c-format
msgid "Cannot spawn low battery level command: %s\n"
msgstr "No se puede ejecutar el comando de nivel de batería baja: %s\n"

#: cbatticon.c:928
msgid "Cannot spawn low battery level command!"
msgstr "¡No se puede ejecutar el comando de nivel de batería baja!"

#: cbatticon.c:938
#, c-format
msgid "Spawning critical battery level command in 30 seconds: %s"
msgstr "Ejecutando el comando de nivel de batería crítico en 30 segundos: %s"

#: cbatticon.c:943
msgid "Skipping critical battery level command, no longer discharging"
msgstr "Omitiendo el comando de nivel de batería crítico, ya no se está descargando"

#: cbatticon.c:949 cbatticon.c:951
#, c-format
msgid "Cannot spawn critical battery level command: %s\n"
msgstr "No se puede ejecutar el comando de nivel de batería crítico: %s\n"

#: cbatticon.c:956
msgid "Cannot spawn critical battery level command!"
msgstr "¡No se puede ejecutar el comando de nivel de batería crítico!"

#: cbatticon.c:971 cbatticon.c:973
#, c-format
msgid "Cannot spawn left click command: %s\n"
msgstr "No se puede ejecutar el comando de clic izquierdo: %s\n"

#: cbatticon.c:978
msgid "Cannot spawn left click command!"
msgstr "¡No se puede ejecutar el comando de clic izquierdo!"

#: cbatticon.c:1042
msgid "Battery is missing!"
msgstr "¡La batería no está presente!"

#: cbatticon.c:1046
msgid "Battery status is unknown!"
msgstr "¡El estado de la batería es desconocido!"

#: cbatticon.c:1050
msgid "Battery is charged!"
msgstr "¡La batería está cargada!"

#: cbatticon.c:1054
#, c-format
msgid "Battery is discharging (%i%% remaining)"
msgstr "La batería se está descargando (%i%% restante)"

#: cbatticon.c:1058
#, c-format
msgid "Battery is not charging (%i%% remaining)"
msgstr "La batería no se está cargando (%i%% restante)"

#: cbatticon.c:1062
#, c-format
msgid "Battery level is low! (%i%% remaining)"
msgstr "¡El nivel de batería es bajo! (%i%% restante)"

#: cbatticon.c:1066
#, c-format
msgid "Battery level is critical! (%i%% remaining)"
msgstr "¡El nivel de batería es crítico! (%i%% restante)"

#: cbatticon.c:1070
#, c-format
msgid "Battery is charging (%i%%)"
msgstr "La batería se está cargando (%i%%)"

