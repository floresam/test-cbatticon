#: cbatticon.c:163
msgid "Display the version"
msgstr "Exibir a versão"

#: cbatticon.c:164
msgid "Display debug information"
msgstr "Exibir informações de depuração"

#: cbatticon.c:165
msgid "Set update interval (in seconds)"
msgstr "Definir intervalo de atualização (em segundos)"

#: cbatticon.c:166
msgid "Set icon type ('standard', 'notification' or 'symbolic')"
msgstr "Definir tipo de ícone (“padrão”, “notificação” ou “simbólico”)"

#: cbatticon.c:167
msgid "Set low battery level (in percent)"
msgstr "Definir nível baixo de bateria (em percentagem)"

#: cbatticon.c:168
msgid "Set critical battery level (in percent)"
msgstr "Definir nível crítico de bateria (em percentagem)"

#: cbatticon.c:169
msgid "Command to execute when low battery level is reached"
msgstr "Comando a executar quando o nível baixo de bateria for atingido"

#: cbatticon.c:170
msgid "Command to execute when critical battery level is reached"
msgstr "Comando a executar quando o nível crítico de bateria for atingido"

#: cbatticon.c:171
msgid "Command to execute when left clicking on tray icon"
msgstr "Comando a executar ao clicar com o botão esquerdo no ícone da bandeja"

#: cbatticon.c:173
msgid "Hide the notification popups"
msgstr "Ocultar as janelas de notificação"

#: cbatticon.c:175
msgid "List available icon types"
msgstr "Listar tipos de ícones disponíveis"

#: cbatticon.c:176
msgid "List available power supplies (battery and AC)"
msgstr "Listar fontes de energia disponíveis (bateria e corrente alternada)"

#: cbatticon.c:180
msgid "[BATTERY ID]"
msgstr "[IDENTIFICADOR DA BATERIA]"

#: cbatticon.c:184
#, c-format
msgid "Cannot parse command line arguments: %s\n"
msgstr "Não é possível analisar os argumentos da linha de comando: %s\n"

#: cbatticon.c:195
msgid ""
"cbatticon: a lightweight and fast battery icon that sits in your system "
"tray\n"
msgstr "cbatticon: um ícone de bateria leve e rápido que se situa na sua bandeja do sistema\n"

#: cbatticon.c:196
#, c-format
msgid "version %s\n"
msgstr "versão %s\n"

#: cbatticon.c:204
msgid "List of available power supplies:\n"
msgstr "Lista de fontes de energia disponíveis:\n"

#: cbatticon.c:219
msgid "List of available icon types:\n"
msgstr "Lista de tipos de ícones disponíveis:\n"

#: cbatticon.c:220 cbatticon.c:221 cbatticon.c:222
msgid "available"
msgstr "disponível"

#: cbatticon.c:220 cbatticon.c:221 cbatticon.c:222
msgid "unavailable"
msgstr "indisponível"

#: cbatticon.c:236
#, c-format
msgid "Unknown icon type: %s\n"
msgstr "Tipo de ícone desconhecido: %s\n"

#: cbatticon.c:248
msgid "No icon type found!\n"
msgstr "Nenhum tipo de ícone encontrado!\n"

#: cbatticon.c:255
#, c-format
msgid "Invalid update interval! It has been reset to default (%d seconds)\n"
msgstr "Intervalo de atualização inválido! Foi redefinido para o padrão (%d segundos)\n"

#: cbatticon.c:262
#, c-format
msgid "Invalid low level! It has been reset to default (%d percent)\n"
msgstr "Nível baixo inválido! Foi redefinido para o padrão (%d por cento)\n"

#: cbatticon.c:267
#, c-format
msgid "Invalid critical level! It has been reset to default (%d percent)\n"
msgstr "Nível crítico inválido! Foi redefinido para o padrão (%d por cento)\n"

#: cbatticon.c:273
msgid ""
"Critical level is higher than low level! They have been reset to default\n"
msgstr "O nível crítico é superior ao nível baixo! Foram redefinidos para o padrão\n"

#: cbatticon.c:367 cbatticon.c:402
#, c-format
msgid "type: %-*.*s\tid: %-*.*s\tpath: %s\n"
msgstr "tipo: %%-*.*s\tid: %%-*.*s\tcaminho: %s\n"

