#: cbatticon.c:163
msgid "Display the version"
msgstr "Afficher la versions"

#: cbatticon.c:164
msgid "Display debug information"
msgstr "Afficher les informations de débogage"

#: cbatticon.c:165
msgid "Set update interval (in seconds)"
msgstr "Définir l'intervalle de mise à jour (en secondes)"

#: cbatticon.c:166
msgid "Set icon type ('standard', 'notification' or 'symbolic')"
msgstr "Définir le type d'icône (« standard », « notification » ou « symbolique »)"

#: cbatticon.c:167
msgid "Set low battery level (in percent)"
msgstr "Définir le niveau de batterie faible (en pourcentage)"

#: cbatticon.c:168
msgid "Set critical battery level (in percent)"
msgstr "Définir le niveau de batterie critique (en pourcentage)"

#: cbatticon.c:169
msgid "Command to execute when low battery level is reached"
msgstr "Commande à exécuter lorsque le niveau de batterie faible est atteint"

#: cbatticon.c:170
msgid "Command to execute when critical battery level is reached"
msgstr "Commande à exécuter lorsque le niveau de batterie critique est atteint"

#: cbatticon.c:171
msgid "Command to execute when left clicking on tray icon"
msgstr "Commande à exécuter lors d'un clic gauche sur l'icône de la barre des tâches"

#: cbatticon.c:173
msgid "Hide the notification popups"
msgstr "Masquer les notifications contextuelles"

#: cbatticon.c:175
msgid "List available icon types"
msgstr "Lister les types d'icônes disponibles"

#: cbatticon.c:176
msgid "List available power supplies (battery and AC)"
msgstr "Lister les sources d'alimentation disponibles (batterie et secteur)"

#: cbatticon.c:180
msgid "[BATTERY ID]"
msgstr "[IDENTIFIANT DE BATTERIE]"

#: cbatticon.c:184
#, c-format
msgid "Cannot parse command line arguments: %s\n"
msgstr "Impossible d'analyser les arguments de la ligne de commande : %s\n"

#: cbatticon.c:195
msgid ""
"cbatticon: a lightweight and fast battery icon that sits in your system "
"tray\n"
msgstr "cbatticon : une icône de batterie légère et rapide qui se loge dans votre barre des tâches\n"

#: cbatticon.c:196
#, c-format
msgid "version %s\n"
msgstr "version %s\n"

#: cbatticon.c:204
msgid "List of available power supplies:\n"
msgstr "Liste des sources d'alimentation disponibles :\n"

#: cbatticon.c:219
msgid "List of available icon types:\n"
msgstr "Liste des types d'icônes disponibles :\n"

#: cbatticon.c:220 cbatticon.c:221 cbatticon.c:222
msgid "available"
msgstr "disponible"

#: cbatticon.c:220 cbatticon.c:221 cbatticon.c:222
msgid "unavailable"
msgstr "indisponible"

#: cbatticon.c:236
#, c-format
msgid "Unknown icon type: %s\n"
msgstr "Type d'icône inconnu : %s\n"

#: cbatticon.c:248
msgid "No icon type found!\n"
msgstr "Aucun type d'icône trouvé!\n"

#: cbatticon.c:255
#, c-format
msgid "Invalid update interval! It has been reset to default (%d seconds)\n"
msgstr "Intervalle de mise à jour invalide ! Il a été réinitialisé à la valeur par défaut (%d secondes)\n"

#: cbatticon.c:262
#, c-format
msgid "Invalid low level! It has been reset to default (%d percent)\n"
msgstr "Niveau faible invalide ! Il a été réinitialisé à la valeur par défaut (%d pour cent)\n"

#: cbatticon.c:267
#, c-format
msgid "Invalid critical level! It has been reset to default (%d percent)\n"
msgstr "Niveau critique invalide ! Il a été réinitialisé à la valeur par défaut (%d pour cent)\n"

#: cbatticon.c:273
msgid ""
"Critical level is higher than low level! They have been reset to default\n"
msgstr "Le niveau critique est supérieur au niveau faible ! Ils ont été réinitialisés aux valeurs par défaut\n"

#: cbatticon.c:367 cbatticon.c:402
#, c-format
msgid "type: %-*.*s\tid: %-*.*s\tpath: %s\n"
msgstr "type : %%-*.*s\tid : %%-*.*s\tchemin : %s\n"

